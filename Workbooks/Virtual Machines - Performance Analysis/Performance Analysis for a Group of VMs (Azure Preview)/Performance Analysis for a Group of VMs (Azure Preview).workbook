    {
      "type": 9,
      "content": {
        "version": "KqlParameterItem/1.0",
        "crossComponentResources": [
          "{TypeR}"
        ],
        "parameters": [
          {
            "id": "f5f90b96-d9d7-4f03-884f-36688f1717bb",
            "version": "KqlParameterItem/1.0",
            "name": "Subscription",
            "type": 6,
            "isRequired": true,
            "value": "value::1",
            "typeSettings": {
              "additionalResourceOptions": [],
              "includeAll": false
            }
          },
          {
            "id": "d82c0b8a-8e0c-484e-b557-0513842508c6",
            "version": "KqlParameterItem/1.0",
            "name": "ResourceGroup",
            "label": "Resource Group",
            "type": 5,
            "isRequired": true,
            "multiSelect": true,
            "quote": "'",
            "delimiter": ",",
            "query": "resourcecontainers\r\n | where type == \"microsoft.resources/subscriptions/resourcegroups\"\r\n | project name\r\n | order by name asc",
            "crossComponentResources": [
              "{Subscription}"
            ],
            "value": [
              "servicemap-monitoring"
            ],
            "typeSettings": {
              "limitSelectTo": 1,
              "additionalResourceOptions": [
                "value::all"
              ],
              "selectAllValue": "All"
            },
            "queryType": 1,
            "resourceType": "microsoft.resourcegraph/resources"
          },
          {
            "id": "2478c313-4fdd-4f99-9418-0008ed823331",
            "version": "KqlParameterItem/1.0",
            "name": "ResourceGroupFilter",
            "type": 1,
            "query": "take 1\r\n| project rgVal = iff(\"{ResourceGroup}\" == \"'All'\", \"\", \"| where resourceGroup in~ ({ResourceGroup:value})\")",
            "isHiddenWhenLocked": true,
            "queryType": 1,
            "resourceType": "microsoft.resourcegraph/resources"
          },
          {
            "id": "3509efa3-a06a-48a5-82da-9d2cf371efca",
            "version": "KqlParameterItem/1.0",
            "name": "ResourceType",
            "label": "Resource Type",
            "type": 2,
            "isRequired": true,
            "value": "microsoft.compute/virtualmachines",
            "typeSettings": {
              "additionalResourceOptions": []
            },
            "jsonData": "[\r\n    {\r\n        \"label\": \"All Compute (VM and VMSS)\",\r\n        \"value\": \"microsoft.resources/resourcegroups\"\r\n    },\r\n    {\r\n        \"label\": \"Virtual machine\",\r\n        \"value\": \"microsoft.compute/virtualmachines\"\r\n    },\r\n    {\r\n        \"label\": \"Virtual machine scale set\",\r\n        \"value\": \"microsoft.compute/virtualmachinescalesets\"\r\n    },\r\n    {\r\n        \"label\": \"Log Analytics workspace\",\r\n        \"value\": \"microsoft.operationalinsights/workspaces\"\r\n    }\r\n]"
          },
          {
            "id": "ece38317-7147-4419-9fd7-ceb0f0e7bb60",
            "version": "KqlParameterItem/1.0",
            "name": "ResourceFilter",
            "type": 1,
            "query": "take 1\r\n| project col = iff(\"{ResourceType}\" == \"microsoft.resources/resourcegroups\", \"| where type in ('microsoft.compute/virtualmachinescalesets', 'microsoft.compute/virtualmachines')\", iff(\"{ResourceType}\" == \"microsoft.compute/virtualmachines\", \"| where type in ('microsoft.compute/virtualmachines')\", iff(\"{ResourceType}\" == \"microsoft.compute/virtualmachinescalesets\", \"| where type in ('microsoft.compute/virtualmachinescalesets')\", iff(\"{ResourceType}\" == \"microsoft.operationalinsights/workspaces\", \"| where type in ('microsoft.operationalinsights/workspaces')\", \"\"))))\r\n",
            "isHiddenWhenLocked": true,
            "queryType": 1,
            "resourceType": "microsoft.resourcegraph/resources"
          },
          {
            "id": "47de52e3-f531-4720-92ce-cd09023fdf37",
            "version": "KqlParameterItem/1.0",
            "name": "ResourceTypeQueryFilter",
            "type": 1,
            "query": "take 1\r\n| project col = iff(\"{ResourceType}\" == \"microsoft.resources/resourcegroups\", \"| where _ResourceId contains 'microsoft.compute/virtualmachines'\", iff(\"{ResourceType}\" == \"microsoft.compute/virtualmachines\", \"| where _ResourceId contains 'microsoft.compute/virtualmachines/'\", iff(\"{ResourceType}\" == \"microsoft.compute/virtualmachinescalesets\", \"| where _ResourceId contains 'microsoft.compute/virtualmachinescalesets'\", iff(\"{ResourceType}\" == \"microsoft.operationalinsights/workspaces\", \"\", \"\"))))",
            "isHiddenWhenLocked": true,
            "queryType": 1,
            "resourceType": "microsoft.resourcegraph/resources"
          },
          {
            "id": "d85d4a03-b559-4d16-9561-7981fa9b551d",
            "version": "KqlParameterItem/1.0",
            "name": "Resource",
            "type": 5,
            "isRequired": true,
            "multiSelect": true,
            "quote": "'",
            "delimiter": ",",
            "query": "resources\r\n{ResourceGroupFilter}\r\n{ResourceFilter}\r\n| summarize by id, name\r\n| project id",
            "crossComponentResources": [
              "{Subscription}"
            ],
            "value": [
              "value::all"
            ],
            "typeSettings": {
              "limitSelectTo": 1,
              "additionalResourceOptions": [
                "value::all"
              ],
              "selectAllValue": "All",
              "showDefault": false
            },
            "queryType": 1,
            "resourceType": "microsoft.resourcegraph/resources"
          },
          {
            "id": "05eb69e6-4f47-47a9-8621-7adfcaed8dd6",
            "version": "KqlParameterItem/1.0",
            "name": "ResourceArray",
            "type": 1,
            "query": "take 1\r\n| project ra = iff(\"{Resource}\" != \"'All'\", \"({Resource})\", \"\")",
            "isHiddenWhenLocked": true,
            "queryType": 1,
            "resourceType": "microsoft.resourcegraph/resources"
          },
          {
            "id": "cc37ca45-0d04-45c4-8fea-cf2fc602f88f",
            "version": "KqlParameterItem/1.0",
            "name": "RG",
            "type": 5,
            "isRequired": true,
            "query": "resourcecontainers\r\n{ResourceGroupFilter}\r\n| project id\r\n",
            "crossComponentResources": [
              "{Subscription}"
            ],
            "value": "value::1",
            "isHiddenWhenLocked": true,
            "typeSettings": {
              "additionalResourceOptions": [
                "value::1"
              ]
            },
            "queryType": 1,
            "resourceType": "microsoft.resourcegraph/resources"
          },
          {
            "id": "43c1df43-dc6d-4516-b642-c30039e1afc0",
            "version": "KqlParameterItem/1.0",
            "name": "ResourceValue",
            "type": 1,
            "query": "take 1\r\n| project col = iff(\"{Resource}\" == \"'All'\", \"microsoft.resources/resourcegroups\", \"{ResourceType:label}\")",
            "crossComponentResources": [
              "{Subscription}"
            ],
            "isHiddenWhenLocked": true,
            "queryType": 1,
            "resourceType": "microsoft.resourcegraph/resources"
          },
          {
            "id": "8d09c704-8a8f-4526-96b3-4658c684d254",
            "version": "KqlParameterItem/1.0",
            "name": "Type",
            "type": 7,
            "query": "take 1\r\n| project column = iff(\"{ResourceGroup:label}\" == \"All\", 'microsoft.resources/subscriptions', iff(isempty(\"{ResourceType}\"), 'microsoft.resources/resourcegroups', iff(\"{ResourceType}\" == \"microsoft.resources/resourcegroups\", 'microsoft.resources/resourcegroups', iff(\"{ResourceType}\" == \"microsoft.compute/virtualmachines\", iff(isnotempty(\"{Resource}\"), iff(\"{Resource}\" != \"'All'\", 'microsoft.compute/virtualmachines', 'microsoft.resources/resourcegroups'), 'microsoft.resources/resourcegroups'), iff(\"{ResourceType}\" == \"microsoft.compute/virtualmachinescalesets\", iff(isnotempty(\"{Resource}\"), iff(\"{Resource}\" != \"'All'\", 'microsoft.compute/virtualmachinescalesets', 'microsoft.resources/resourcegroups'), 'microsoft.resources/resourcegroups'), iff(\"{ResourceType}\" == \"microsoft.operationalinsights/workspaces\", iff(isnotempty(\"{Resource}\"), iff(\"{Resource}\" != \"'All'\", \r\n'microsoft.operationalinsights/workspaces', 'microsoft.resources/resourcegroups'), 'microsoft.resources/resourcegroups'), 'microsoft.resources/resourcegroups'))))))\r\n",
            "crossComponentResources": [
              "{Subscription}"
            ],
            "value": "value::1",
            "isHiddenWhenLocked": true,
            "typeSettings": {
              "additionalResourceOptions": [
                "value::1"
              ]
            },
            "queryType": 1,
            "resourceType": "microsoft.resourcegraph/resources"
          },
          {
            "id": "184e49af-8134-45be-855c-a99b3462531b",
            "version": "KqlParameterItem/1.0",
            "name": "TypeR",
            "type": 5,
            "query": "take 1\r\n| project resource = iff(\"{Type}\" == \"microsoft.resources/subscriptions\", {Subscription}, iff(\"{Type}\" == \"microsoft.resources/resourcegroups\", \"{RG}\", iff(\"{Type}\" == \"microsoft.compute/virtualmachines\", {Resource}, iff(\"{Type}\" == \"microsoft.compute/virtualmachinescalesets\", pack_array({Resource}), iff(\"{Type}\" == \"microsoft.operationalinsights/workspaces\", {Resource}, \"{RG}\")))))",
            "value": "value::1",
            "isHiddenWhenLocked": true,
            "typeSettings": {
              "additionalResourceOptions": [
                "value::1"
              ]
            },
            "queryType": 1,
            "resourceType": "microsoft.resourcegraph/resources"
          },
          {
            "id": "70f73d2d-624e-41ea-9f48-60afed064adc",
            "version": "KqlParameterItem/1.0",
            "name": "ResourceQueryFilter",
            "type": 1,
            "query": "take 1\r\n| project resource = iff(isnotempty(\"{ResourceArray}\"), \"| where _ResourceId in~ {ResourceArray}\", \"\")",
            "crossComponentResources": [
