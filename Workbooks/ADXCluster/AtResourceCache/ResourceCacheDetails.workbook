                    }
                  },
                  {
                    "columnMatch": "CacheSize",
                    "formatter": 4,
                    "formatOptions": {
                      "palette": "blue",
                      "customColumnWidthSetting": "15ch"
                    },
                    "numberFormat": {
                      "unit": 2,
                      "options": {
                        "style": "decimal"
                      }
                    }
                  }
                ],
                "rowLimit": 350,
                "filter": true,
                "sortBy": [
                  {
                    "itemKey": "DatabaseName",
                    "sortOrder": 1
                  }
                ],
                "labelSettings": [
                  {
